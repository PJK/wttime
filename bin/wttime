#!/usr/bin/env python3

import click
from datetime import datetime
from pytz import timezone
import pytz
from wttime.parser import Parser

@click.command()

@click.argument('timestring')
def parse(timestring):
    confidence, guess = Parser().parse(datetime.now(), timestring)
    print(guess.replace(tzinfo=pytz.utc).strftime('%Y-%m-%d %H:%M:%S %Z%z'))
    print(guess.strftime('%Y-%m-%d %H:%M:%S %Z%z'))


if __name__ == '__main__':
    parse()